<h1>Listing delivery_areas</h1>

<table>
  <tr>
    <th>Name</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @delivery_areas.each do |delivery_area| %>
  <tr>
    <td><%= delivery_area.name %></td>
    <td><%= delivery_area.user %></td>
    <td><%= link_to 'Show', delivery_area %></td>
    <td><%= link_to 'Edit', edit_delivery_area_path(delivery_area) %></td>
    <td><%= link_to 'Destroy', delivery_area, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

