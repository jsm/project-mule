= form_for(@delivery_point) do |f|
  - if @delivery_point.errors.any?
    #error_explanation
      %h2
        = pluralize(@delivery_point.errors.count, "error")
        prohibited this delivery_point from being saved:
      %ul
        - @delivery_point.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :city
    %br/
    = f.text_field :city
  .field
    = f.label :line1
    %br/
    = f.text_field :line1
  .field
    = f.label :line2
    %br/
    = f.text_field :line2
  .field
    = f.label :state
    %br/
    = f.text_field :state
  .field
    = f.label :zip
    %br/
    = f.text_field :zip
  .field
    = f.label :latitude
    %br/
    = f.text_field :latitude
  .field
    = f.label :longitude
    %br/
    = f.text_field :longitude

  .actions
    = f.submit
